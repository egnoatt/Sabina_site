      "@router.get('/health')",
      "def health():",
      "    return {'status': 'ok'}",
      "",
      "@router.post('/items')",
      "def create_item(item: dict):",
      "    if not item:",
      "        raise HTTPException(status_code=400, detail='Invalid item')",
      "    return {'created': item}",
      ""
    ],
    "description": "Simple FastAPI router with health and POST"
  }
